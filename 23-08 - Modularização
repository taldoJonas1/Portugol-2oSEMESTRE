2. VERIFICAR SE O NRO É PRIMO OU NÃO

==================================================================================================

3. CALCULAR O QUOCIENTE INTEIRO DA DIVISAO ENTRE DOIS NROS, SEM UTILIZAR OPERADOR DIV

programa
{
	
	funcao inicio()
	{
		inteiro A, B, N
		escreva("Insira o dividendo: ")
		leia(A)
		escreva("Agora insira o divisor: ")
		leia(B)
		N = div(A, B)
		escreva("Resultado da divisão sem o resto: ",N)
	}

	funcao inteiro div(inteiro A, inteiro B){
		inteiro cont=0
		enquanto(A>=B){
			cont++
			A = A - B
		}
		retorne cont
	}
}

==================================================================================================

4. CALCULAR O FATORIAL DE UM NRO

programa
{
	
	funcao inicio()
	{
		inteiro N, X
		escreva("Entre com um nro: ")
		leia(N)
		X = fat(N)
		escreva(N,"! = ", X)
	}
	funcao inteiro fat(inteiro N){
		inteiro fatorial = 1
		enquanto(N>0){
		fatorial = fatorial * N
		N--
		}
		retorne fatorial
	}
}
